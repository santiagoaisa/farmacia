<%@ taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c" %>

.z-decimalbox { 
font-size:11px;
}

.z-decimalbox-focus { 
    border-style: solid;
    border-width: 1px;
    border-color: #000000;
    color : #000000 !important;
    background-repeat : no-repeat;
}

.z-decimalbox {
    text-align:right;
}

/* Decimalbox: Disabled / readonly background color */
.z-decimalbox-readonly,.z-decimalbox-text-disd {
    background: #ECEAE4;
}
