<%@ taglib uri="http://www.zkoss.org/dsp/web/core" prefix="c" %>

.z-intbox { 
font-size:11px;
}

.z-intbox-focus { 
    border-style: solid;
    border-width: 1px;
    border-color: #000000;
    color : #000000 !important;
    background-repeat : no-repeat;
}

.z-intbox {
    text-align:right;
}

/* Decimalbox: Disabled / readonly background color */
.z-intbox-readonly,.z-intbox-text-disd {
    background: #ECEAE4;
}
