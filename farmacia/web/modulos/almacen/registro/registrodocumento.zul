<?init class="org.zkoss.zkplus.databind.AnnotateDataBinderInit" arg0="./winDocumentoFinanzas"?>
<window id="winDocumentoFinanzas" position="center" title="Registro de Proveedores..."
apply="modalmacen.registro.RegistroDocumento" border="0" width="460px"  >

  <grid fixedLayout="true" width="100%" >
        <columns >
            <column width="22%" />
            <column width="56%" />
            <column width="22%" />

        </columns>
        <rows>
            <row>               
                    <label value="Proveedor:" />
                    <textbox id="txtNombre"  width="95%" readonly="true" />
                    <textbox id="txtRuc"  width="90%" readonly="true" />
            </row>
        </rows>
    </grid>

    <grid fixedLayout="true" width="100%">
        <auxhead>
            <auxheader label="Datos de Documentos" colspan="5" />
        </auxhead>
        <columns >
            <column label="Tipo" width="22%" />
            <column Label="Serie" width="10%" />
            <column Label="Número" width="20%" />
            <column Label="" width="16%" />
            <column Label="Serie" width="10%" />
            <column Label="Número" width="22%" />
        </columns>
        <rows>
            <row >
                  <combobox id = "cboDocumento" width="95%" readonly="true"  constraint="no empty:Debe Seleccionar Documento" />
                 <textbox id="txtSerfac"  maxlength="3" width="85%" />
                 <textbox id="txtNumfac"  maxlength="10" width="85%" />
                 <label value="Guia:"/>
                 <textbox id="txtSergui"  maxlength="3" width="85%" />
                 <textbox id="txtNumgui"  maxlength="10" width="85%" />
                      
            </row>
        </rows>
    </grid>
    <grid fixedLayout="true" width="100%">
        <columns>
            <column width="22%" />
            <column width="22%" />
            <column width="10%" />
            <column width="10%" />
            <column width="14%" />
            <column width="22%" />
        </columns>
        <rows>
            <row>                
                    <label value="Fecha Emisión:" />
                    <datebox id="dFecemi" readonly="true" width="95%" format="yyyy-MM-dd"  />
                    <label value="Plazo:" />
                    <intbox id="nPlazo" width="95%" />
                    <label value="Vence:" />
                    <datebox id="dFecven" readonly="true" width="95%" format="yyyy-MM-dd" disabled="true" />
               
            </row>
        </rows>
    </grid>


    <grid fixedLayout="true" width="100%">
        <columns>
            <column width="33%" />
            <column width="17%" />
            <column width="33%" />
            <column width="17%" />
        </columns>
        <rows>
            <row>                
                <label value="V.V. Afecto: " />
                <decimalbox id="nAfecto" value="0.00" format="###,###,##0.00"  width="95%" />
                <label value="V.V. Inafecto:" />
                <decimalbox id="nInafecto" value="0.00" format="###,###,##0.00" width="95%" />
            </row>
    
            <row>
                <hbox>
                    <label value="IGV" />
                    <textbox id="txtIgv" value="18%" width="35px" readonly="true" />
                </hbox>
                <decimalbox id="nIgv" value="0.00" format="###,###,##0.00"  width="95%" readonly="false" />
                <label value="Precio Venta:" />
                <decimalbox id="nPreven" value="0.00" format="###,###,##0.00"  width="95%" readonly="false" />

            </row>
    
            <row>
                <label value="Percepción:" />                
                <decimalbox id="nPercepcion" value="0.00" format="###,###,##0.00" width="95%" />
                <label value="Rec. faltante Mercaderia:" />
                <decimalbox id="nDevolucion" value="0.00" format="###,###,##0.00" width="95%" />

            </row>
    
            <row>
                <label value="Rec. diferencia de Precio:" />
                <decimalbox id="nDescuento" value="0.00" format="###,###,##0.00" width="95%" />
               
                <label value="Diferencia de Reclamo:" />
                <decimalbox id="nDiferencia" value="0.00" format="###,###,##0.00" width="95%" readonly="true"/>
                
            </row>
    
            <row spans="1,1,2">
                 <label value="Total Ingreso" />
                <decimalbox id="nIngreso" value="0.00"  format="###,###,##0.00"  width="95%" readonly="true" />
                   <label  />  
                
            </row>
            <row >
              
               <label value="Moneda:" />
                <combobox id = "cboMoneda" width="95%" readonly="true"  constraint="no empty:Debe Seleccionar Moneda" />
                
               <label value="Tipo Cambio" />
                <decimalbox id="nTcambio" value="1.00"  format="###,###,##0.0000"  width="95%" />

            </row>
            <row spans="4">
                <groupbox >
                            <caption>Observaciones</caption>
                            <textbox id="txtObservacion" multiline="true" height="40px" width="99%"/>
                </groupbox>
            </row>
        </rows>
    </grid>

    


        <toolbar id="tb"  height="25px" align="end"
			style="background:#defeff repeat-x 0 0;padding-top:7px">
            <toolbarbutton id="btnGrabar" label="Registrar" image="/img/Save.png"
			 />      
        </toolbar>


    <zscript>
    {
        void doCancel() {
        winDocumentoFinanzas.setAttribute("REST",false);
        winDocumentoFinanzas.onClose();
        }
    }
    </zscript>

</window>